{
  "name": "prisma-schema",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/prisma-schema/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project prisma-schema --web",
  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "{projectRoot}"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "{projectRoot}"
      }
    },
    "generate-types": {
      "command": "prisma generate --no-engine",
      "options": {
        "cwd": "{projectRoot}"
      }
    },
    "reset": {
      "command": "prisma migrate reset",
      "options": {
        "cwd": "{projectRoot}"
      }
    },
    "start": {
      "command": "prisma dev",
      "options": {
        "cwd": "{projectRoot}"
      }
    },
    "seed": {
      "command": "npx ts-node libs/prisma-schema/src/seed.ts",
      "options": {
        "cwd": "{projectRoot}"
      }
    }
  }
}
